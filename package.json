{
  "name": "proxy",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "nodemon -L --exec ts-node -- src/index.ts",
    "format": "prettier --config .prettierrc --ignore-path .gitignore --write '**/*.{ts,js,json}'",
    "lint-staged": "lint-staged"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": [
      "prettier --config .prettierrc --write --",
      "git add"
    ],
    "*.{js,json}": [
      "prettier --config .prettierrc --write --",
      "git add"
    ]
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/axios": "^0.14.0",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.2",
    "@types/jsdom": "^12.2.4",
    "@types/mongodb": "^3.3.15",
    "axios": "^0.19.2",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-request-proxy": "^2.2.2",
    "jsdom": "^16.1.0",
    "mongodb": "^3.5.2",
    "typescript": "^3.7.5"
  },
  "devDependencies": {
    "husky": "^4.2.1",
    "lint-staged": "^10.0.7",
    "nodemon": "^2.0.2",
    "prettier": "^1.19.1",
    "ts-node": "^8.6.2"
  }
}
